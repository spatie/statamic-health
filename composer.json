{
    "name": "spatie/statamic-health",
    "description": "Check the health of your Statamic app",
    "keywords": [
        "spatie",
        "laravel",
        "statamic-health"
    ],
    "homepage": "https://github.com/spatie/statamic-health",
    "license": "MIT",
    "authors": [
        {
            "name": "Rias",
            "email": "rias@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "statamic/cms": "^3.2",
        "spatie/laravel-health": "^1.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\StatamicHealth\\": "src",
            "Spatie\\StatamicHealth\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\StatamicHealth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "statamic": {
            "name": "Health",
            "description": "Check the health of your Statamic app"
        },
        "laravel": {
            "providers": [
                "Spatie\\StatamicHealth\\StatamicHealthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
